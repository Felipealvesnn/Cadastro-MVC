@using Cadastro.Dominio.Models
@model IEnumerable<Cadastro.Dominio.Models.Cliente>
@{
    ViewBag.Title = "Index";
}

<h2>Index do cliente</h2>

<div>
    <a href="Adicionar" class="btn btn-success">Novo Cleinte</a>
</div>

<table class="table table-striped">
    <thead>
        <tr>
            <th> Razão Social</th>
            <th>Nome Fantasia</th>
            <th>CNPJ</th>
            <th>Endereço </th>
            <th>Logradouro</th>
            <th>Número</th>
            <th>Bairro</th>
            <th>Complemento</th>
            <th>Municipio</th>
            <th>Cep</th>
        </tr>
    </thead>

    <tbody>
        @foreach (Cliente Clientes in Model)
        {
            <tr>
                <td>@Clientes.Razsocial</td>
                <td>@Clientes.NomeFan</td>
                <td>@Clientes.Cnpj</td>
                <td>@Clientes.Endereco</td>
                <td>@Clientes.Numero</td>
                <td>@Clientes.Bairro</td>
                <td>@Clientes.Complemento</td>
                <td>@Clientes.Municipio</td>
                <td>@Clientes.Cep</td>
               
               
                <td>
                    <a href="Editar/@Clientes.Id" class="btn btn-primary">Editar</a>
                    
                    <a href="Remover/@Clientes.Id" class="btn btn-danger">Remover</a>
                </td>
            </tr>
        }


    </tbody>
</table>